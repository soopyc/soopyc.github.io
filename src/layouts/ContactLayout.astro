---
import type { Link } from "../assets/contactLinks";
import ContactLink from "../components/ContactLink.astro";

const [section, links]: [string, Link[]] = Astro.props.section;
// TODO: make this ItemFarm or something and dedup impl of ContactLink and the games item farm
---

<h2>{section}</h2>
<div>
    {links.map(link => <ContactLink {link} />)}
</div>

<style>
    div {
        display: flex;
        flex-wrap: wrap;
        column-gap: 1ch;
    }
</style>
